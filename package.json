{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React Starter",
  "main": "main.js",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "startp": "webpack-dev-server -p --progress --config ./webpack.production.config.js",
    "build": "NODE_ENV=production webpack --config ./webpack.production.config.js --progress",
    "clean": "rm build/*",
    "lint": "eslint . --ext .js --ext .jsx",
    "test": "node_modules/karma/bin/karma start",
    "test_jasmine": "jasmine JASMINE_CONFIG_PATH=test/jasmine.json",
    "cov_mocha": "babel-node node_modules/.bin/istanbul cover _mocha -- 'test/*_test.js'",
    "cov_jasmine": "node_modules/.bin/istanbul cover jasmine JASMINE_CONFIG_PATH=test/jasmine.json"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "^0.4.4",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^1.8.0",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.0",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achristoph/react-starter.git"
  },
  "keywords": [
    "react",
    "starter"
  ],
  "author": "Arthur Christoph",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/achristoph/react-starter/issues"
  },
  "homepage": "https://github.com/achristoph/react-starter#readme"
}
