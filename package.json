{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React Starter",
  "main": "main.js",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "startp": "webpack-dev-server -p --progress --config ./webpack.production.config.js",
    "build": "NODE_ENV=production webpack --config ./webpack.production.config.js --progress",
    "clean": "rm build/*",
    "lint": "eslint . --ext .js --ext .jsx",
    "test": "node_modules/karma/bin/karma start",
    "test_jasmine": "jasmine JASMINE_CONFIG_PATH=test/jasmine.json",
    "cov_mocha": "babel-node node_modules/.bin/istanbul cover _mocha -- 'test/*_test.js'",
    "cov_jasmine": "node_modules/.bin/istanbul cover jasmine JASMINE_CONFIG_PATH=test/jasmine.json"
  },
  "author": "Arthur Christoph",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.6.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.3.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.10.0",
    "istanbul": "^0.4.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.7",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.5",
    "postcss-loader": "^0.8.1",
    "react-addons-test-utils": "^0.14.7",
    "react-transform-hmr": "^1.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
